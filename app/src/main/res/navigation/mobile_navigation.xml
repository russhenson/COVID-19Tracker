<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_city">

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.mobdeve.hensonruss.covid_19tracker.ui.dashboard.DashboardFragment"
        tools:layout="@layout/fragment_dashboard" />

    <fragment
        android:id="@+id/navigation_city"
        android:name="com.mobdeve.hensonruss.covid_19tracker.ui.city.CityFragment"
        tools:layout="@layout/fragment_city" />

    <fragment
        android:id="@+id/navigation_symptoms"
        android:name="com.mobdeve.hensonruss.covid_19tracker.ui.symptoms.SymptomsFragment"
        tools:layout="@layout/fragment_symptoms" />

    <fragment
        android:id="@+id/navigation_hotline"
        android:name="com.mobdeve.hensonruss.covid_19tracker.ui.hotline.HotlineFragment"
